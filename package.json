{
  "name": "timdeschryver.dev",
  "author": "Tim Deschryver",
  "description": "timdeschryver.dev",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "prebuild": "node --version && npm run copy:blog && npm run copy:snippets",
    "build": "sapper export --legacy --entry \"/ /resources/ngrx /blog/latest\"",
    "build:lambda": "rollup -c rollup.lambda.config.js",
    "copy:blog": "rimraf static/blog && copyfiles \"content/blog/**\" static -u 1",
    "copy:snippets": "rimraf static/snippets && copyfiles \"content/snippets/**\" static -u 1",
    "start": "node __sapper__/build",
    "start:serve": "serve __sapper__/export -l 3000",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "test-against-build": "run-p --race start:serve cy:run",
    "publish:devto": "node ./scripts/devto",
    "g": "node ./scripts/generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && lint-staged"
    }
  },
  "dependencies": {
    "@polka/send": "^0.4.0",
    "@sveltejs/site-kit": "^1.2.4",
    "apollo-boost": "0.4.9",
    "apollo-server": "^2.16.1",
    "apollo-server-express": "^2.16.1",
    "apollo-server-micro": "^2.16.1",
    "compression": "^1.7.4",
    "gatsby-remark-prismjs": "^3.5.10",
    "gatsby-transformer-remark": "^2.8.28",
    "glob": "^7.1.6",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "marked": "^1.1.1",
    "polka": "^0.5.2",
    "prettier": "^2.0.5",
    "prismjs": "^1.21.0",
    "remark": "^12.0.1",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "@testing-library/cypress": "^6.0.0",
    "axe-core": "^4.0.1",
    "copyfiles": "^2.3.0",
    "cypress": "^4.12.1",
    "cypress-axe": "^0.8.1",
    "dotenv-extended": "^2.8.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "rollup": "^2.26.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "rxjs": "^6.6.2",
    "sapper": "^0.28.0",
    "serve": "^11.3.2",
    "svelte": "^3.24.1",
    "svelte-rx": "0.0.7",
    "svelte-xstate-stores": "0.0.6",
    "xstate": "^4.12.0"
  },
  "license": "MIT"
}
