{
  "name": "timdeschryver.dev",
  "author": "Tim Deschryver",
  "description": "timdeschryver.dev",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "prebuild": "node --version && npm run copy:blog && npm run copy:snippets",
    "build": "sapper export --legacy --timeout=60000",
    "build:lambda": "rollup -c rollup.lambda.config.js",
    "copy:blog": "rimraf static/blog && copyfiles \"content/blog/**\" static -u 1",
    "copy:snippets": "rimraf static/snippets && copyfiles \"content/snippets/**\" static -u 1",
    "start": "node __sapper__/build",
    "start:serve": "serve __sapper__/export -l 3000",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "test-against-build": "run-p --race start:serve cy:run",
    "publish:devto": "node ./scripts/devto"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && lint-staged"
    }
  },
  "dependencies": {
    "@polka/send": "^0.4.0",
    "@sveltejs/site-kit": "^1.0.4",
    "@testing-library/cypress": "^5.3.0",
    "apollo-boost": "0.4.2",
    "apollo-server": "^2.6.3",
    "apollo-server-express": "^2.6.3",
    "apollo-server-micro": "^2.6.3",
    "compression": "^1.7.1",
    "cypress": "^4.0.2",
    "gatsby-remark-prismjs": "^3.3.20",
    "gatsby-transformer-remark": "^2.6.30",
    "glob": "^7.1.4",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "marked": "^0.6.2",
    "polka": "^0.5.0",
    "prismjs": "^1.16.0",
    "remark": "^11.0.1",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "axe-core": "^3.5.1",
    "copyfiles": "^2.1.0",
    "cypress-axe": "^0.5.3",
    "date-fns": "^1.30.1",
    "dotenv-extended": "^2.4.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-svelte3": "^1.1.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rxjs": "^7.0.0-alpha.1",
    "sapper": "^0.27.9",
    "serve": "^11.3.0",
    "svelte": "^3.6.9",
    "svelte-rx": "0.0.7",
    "svelte-xstate-stores": "0.0.6",
    "xstate": "^4.7.5"
  },
  "license": "MIT"
}
