{
	"name": "timdeschryver.dev",
	"author": "Tim Deschryver",
	"description": "timdeschryver.dev",
	"version": "0.0.1",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": "^16.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"prebuild": "rimraf build",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"g": "node ./scripts/generate",
		"p": "node ./scripts/devto",
		"banners": "node ./scripts/banners.js",
		"pre-commit": "npm run banners && git add ./content/**/banner.jpg && lint-staged",
		"optimize:image": "node ./scripts/optimize-image-cli.js",
		"optimize:images": "node ./scripts/optimize-images.js",
		"prepare": "git config core.hookspath .githooks && svelte-kit sync"
	},
	"devDependencies": {
		"@azure/static-web-apps-cli": "1.0.2",
		"@playwright/test": "1.24.0",
		"@squoosh/lib": "0.4.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"@types/marked": "4.0.3",
		"@typescript-eslint/eslint-plugin": "5.30.7",
		"@typescript-eslint/parser": "5.30.7",
		"axe-playwright": "1.1.11",
		"copyfiles": "2.4.1",
		"dotenv-extended": "2.9.0",
		"esbuild": "0.14.53",
		"eslint": "8.20.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"front-matter": "4.0.2",
		"gatsby-remark-prismjs": "6.19.0",
		"lint-staged": "13.0.3",
		"marked": "4.0.18",
		"node-fetch": "3.2.10",
		"playwright": "1.24.0",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "2.7.0",
		"prismjs": "1.28.0",
		"rimraf": "3.0.2",
		"svelte": "3.49.0",
		"svelte-check": "2.8.0",
		"svelte-preprocess": "4.10.7",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vite": "3.0.2",
		"vite-plugin-restart": "^0.2.0"
	}
}
